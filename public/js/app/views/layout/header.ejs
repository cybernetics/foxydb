<header>
	<a class="logo" href="#!dashboard">FoxyDB</a>
	<div class="tabs" <%= (el) -> new Controller.Tabs(el) %>>

	</div>
	<% if(Global.state.attr('loggedIn')) { %>
		<span class="user"><%= Global.user.attr('name') %> <i class="fa fa-angle-down "></i>
			<ul>
				<% if(Global.user.attr('level') == 0) { %>
					<li><a href="#!users"><i class="fa fa-user"></i> Manage Users</a></li>
					<li><a href="#!database/list"><i class="fa fa-bars"></i> Manage Databases</a></li>
				<% } %>
				<li><a href="#!account/logout"><i class="fa fa-sign-out"></i> Log Out</a></li>
				
				<% if (typeof config.baseURL !== 'undefined' && config.baseURL.length) { %>
					<li><a href="<%=config.baseURL%>"><i class="fa fa-home"></i> Back to Site</a></li>
				<% } %>
			</ul>
		</span>
	<% } %>
</header>
<div class="errorMessage" ></div>