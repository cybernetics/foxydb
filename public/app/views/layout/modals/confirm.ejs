<div class="confirmModal ui small modal">
    <div class="header">
      <%= title%>
    </div>
    <div class="content">
      <p><%= content%></p>
    </div>
    <div class="actions">
      <div class="ui negative button">
        <%= negative%>
      </div>
      <div class="ui positive right labeled icon button">
        <%= positive%>
        <i class="checkmark icon"></i>
      </div>
    </div>
  </div>