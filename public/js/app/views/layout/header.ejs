<header>
	<a class="logo" href="#!dashboard">FoxyDB</a>
	<div class="tabs" <%= (el) -> new Controller.Tabs(el) %>>

	</div>
	<% if(Global.state.attr('loggedIn')) { %>
		<span class="user"><%= Global.user.attr('name') %> <i class="fa fa-angle-down "></i>
			<ul>
				<li><a href="/account/logout"><i class="fa fa-sign-out"></i> Log out</a>
			</ul>
		</span>
	<% } %>
</header>